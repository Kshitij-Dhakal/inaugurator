{
    "name": "ng",
    "commands": [
        {
            "command": "generate",
            "alias": "g",
            "subcommands": [
                {
                    "command": "component",
                    "alias": "c",
                    "templates": [
                        {
                            "template": "tmp/name.component.ts",
                            "out": "__ss__name/__lkc__name.component.ts"
                        },
                        {
                            "template": "tmp/name.component.html",
                            "out": "__ss__name/__lkc__name.component.ts"
                        }
                    ]
                },
                {
                    "command": "service",
                    "alias": "s",
                    "templates": [
                        {
                            "template": "tmp/name.service.ts",
                            "out": "__lkc__name.service.ts"
                        }
                    ]
                }
            ]
        }
    ]
}